<!DOCTYPE html>
<html>
  <head>
    <title>TaskListProject</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "https://stackpath.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css" %>
    <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
    <%= javascript_include_tag "https://stackpath.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js" %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%#= javascript_include_tag  'shared' %>
  </head>
   
  <body>
    <%= render partial: "layouts/header" %>
    <%= render partial: 'layouts/notice' %>
    <main id="main" role="main container">
    <%= yield %>
    </main>
    <%= render partial: "layouts/footer" %>
  </body>
</html>
