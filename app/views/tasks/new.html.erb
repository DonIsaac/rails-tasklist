<p id="notice"><%= notice %></p>
<h1>New Task</h1>

<%#= render 'form', task: @task %>
 <%= semantic_form_for @task do |f| %>
    <%= f.inputs :name, :description %>
	<%= f.input :task_list_id, :as => :hidden, :input_html => {:value => params[:list]} %>
    <%= f.actions :submit, :cancel %>
<% end %>

<%= link_to 'Back', task_list_url(params[:list]) %>
